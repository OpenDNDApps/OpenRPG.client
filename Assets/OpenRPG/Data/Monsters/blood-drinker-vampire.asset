    armor"]}],"hp":{"average":90,"formula":"12d8 + 36"},"speed":{"walk":40,"fly":{"number":40,"condition":"(hover)"},"canHover":true},"str":16,"dex":18,"con":17,"int":16,"wis":13,"cha":19,"save":{"dex":"+7","con":"+6","wis":"+4"},"skill":{"intimidation":"+7","perception":"+4","stealth":"+7"},"senses":["darkvision
