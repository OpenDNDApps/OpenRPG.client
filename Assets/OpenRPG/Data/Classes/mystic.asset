  Title: Mystic
  AsJson: "{\"name\":\"Mystic\",\"source\":\"UATheMysticClass\",\"page\":1,\"hd\":{\"number\":1,\"faces\":8},\"proficiency\":[\"int\",\"wis\"],\"startingProficiencies\":{\"armor\":[\"light\"],\"weapons\":[\"simple\"],\"tools\":[\"None\"],\"skills\":[{\"choose\":{\"from\":[\"arcana\",\"history\",\"insight\",\"medicine\",\"nature\",\"perception\",\"religion\"],\"count\":2}}]},\"startingEquipment\":{\"additionalFromBackground\":true,\"default\":[\"(a)
