  AsJson: '{"name":"Charm Person","source":"PHB","page":221,"srd":true,"basicRules":true,"level":1,"school":"E","time":[{"number":1,"unit":"action"}],"range":{"type":"point","distance":{"type":"feet","amount":30}},"components":{"v":true,"s":true},"duration":[{"type":"timed","duration":{"type":"hour","amount":1}}],"entries":["You
