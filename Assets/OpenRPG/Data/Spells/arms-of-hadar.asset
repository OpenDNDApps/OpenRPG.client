  AsJson: '{"name":"Arms of Hadar","source":"PHB","page":215,"level":1,"school":"C","time":[{"number":1,"unit":"action"}],"range":{"type":"radius","distance":{"type":"feet","amount":10}},"components":{"v":true,"s":true},"duration":[{"type":"instant"}],"entries":["You
