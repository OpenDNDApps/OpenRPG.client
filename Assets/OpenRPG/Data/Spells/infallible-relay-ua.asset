  AsJson: '{"name":"Infallible Relay (UA)","source":"UAModernMagic","page":7,"level":1,"school":"D","time":[{"number":1,"unit":"minute"}],"range":{"type":"point","distance":{"type":"self"}},"components":{"v":true,"s":true,"m":"a
    mobile phone"},"duration":[{"type":"timed","duration":{"type":"minute","amount":10},"concentration":true}],"meta":{"technomagic":true},"entries":["With
