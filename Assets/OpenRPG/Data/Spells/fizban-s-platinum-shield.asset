  AsJson: '{"name":"Fizban''s Platinum Shield","source":"UA2021DraconicOptions","page":5,"level":6,"school":"A","time":[{"number":1,"unit":"action"}],"range":{"type":"point","distance":{"type":"feet","amount":60}},"components":{"v":true,"s":true,"m":{"text":"a
