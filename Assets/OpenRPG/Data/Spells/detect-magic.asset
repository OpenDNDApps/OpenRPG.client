  AsJson: '{"name":"Detect Magic","source":"PHB","page":231,"srd":true,"basicRules":true,"level":1,"school":"D","time":[{"number":1,"unit":"action"}],"range":{"type":"point","distance":{"type":"self"}},"components":{"v":true,"s":true},"duration":[{"type":"timed","duration":{"type":"minute","amount":10},"concentration":true}],"meta":{"ritual":true},"entries":["For
