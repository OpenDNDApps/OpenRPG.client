  AsJson: '{"name":"Summon Shadowspawn","source":"TCE","page":113,"level":3,"school":"C","time":[{"number":1,"unit":"action"}],"range":{"type":"point","distance":{"type":"feet","amount":90}},"components":{"v":true,"s":true,"m":{"text":"tears
