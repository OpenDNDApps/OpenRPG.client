  AsJson: '{"name":"Locathah","source":"LR","page":24,"size":["M"],"speed":{"walk":30,"swim":30},"ability":[{"str":2,"dex":1}],"age":{"mature":10,"max":80},"traitTags":["Amphibious","Natural
