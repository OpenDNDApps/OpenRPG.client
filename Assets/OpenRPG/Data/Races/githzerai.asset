  AsJson: '{"name":"Githzerai","source":"MPMM","page":19,"lineage":"VRGR","size":["M"],"speed":30,"resist":["psychic"],"additionalSpells":[{"innate":{"3":{"daily":{"1":["shield"]}},"5":{"daily":{"1":["detect
