  AsJson: '{"name":"Elf (Zendikar)","source":"PSZ","page":18,"size":["M"],"speed":30,"ability":[{"wis":2}],"age":{"mature":100,"max":750},"darkvision":60,"skillProficiencies":[{"perception":true}],"languageProficiencies":[{"common":true,"elvish":true}],"entries":[{"name":"Age","type":"entries","entries":["Although
